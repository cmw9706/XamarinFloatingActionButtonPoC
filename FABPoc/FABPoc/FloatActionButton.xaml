    xmlns:controls="clr-namespace:FFImageLoading.Svg.Forms;assembly=FFImageLoading.Svg.Forms"
    x:Class="FABPoc.FloatActionButton">
    <ContentView.Content>
        <Frame BackgroundColor="#291972" CornerRadius="8">
            <controls:SvgCachedImage Source="resource://FABPoc.Vector.svg" HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand"></controls:SvgCachedImage>
        </Frame>
    </ContentView.Content>
</ContentView>

